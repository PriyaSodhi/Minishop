<% flash.each do |message_type, message| %>
  <% if message_type == "success" %>
    <div class="alert alert-success">
      <ul>
        <li><%= message %> </li>
      </ul>
  <% else %>
    <div class="alert alert-danger">
      <ul>
        <li><%= message %></li>
      </ul>
    </div>
  <% end%>
<% end %>
